<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffefd5"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:background="@drawable/home_draw"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:text="A.young_B.old"
            android:textColor="#E8605A50"
            android:textSize="24sp"
            android:textStyle="bold|italic" />

        <Button
            android:id="@+id/profileBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#CABEAA"
            android:text="profile" />

        <Button
            android:id="@+id/plannerBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#CABEAA"
            android:text="planner" />

        <Button
            android:id="@+id/diaryBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#CABEAA"
            android:text="diary" />

        <Button
            android:id="@+id/studyBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#CABEAA"
            android:text="study" />

        <Button
            android:id="@+id/guestBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#CABEAA"
            android:text="guest" />
    </LinearLayout>

</FrameLayout>